{
  "name": "s42-interface",
  "private": true,
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=18.0.0"
  },
  "scripts": {
    "generate": "yarn run generate:gql && yarn run generate:campus",
    "generate:gql": "graphql-codegen --config graphqlcodegen.yml",
    "generate:campus": "ts-node generators/generateCampusTypes.ts",
    "dev": "next dev",
    "dev:watch": "concurrently \"yarn run dev\" \"yarn run generate --watch\"",
    "build": "next build",
    "start": "next start",
    "style:all": "yarn run style:lint && yarn run style:prettier&& yarn run style:ts",
    "style:lint": "next lint",
    "style:ts": "yarn run tsc -t es6",
    "style:prettier": "prettier --check '**/*.{js,jsx,ts,tsx}'",
    "make-pretty": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "test": "jest --watch",
    "test:ci": "jest --passWithNoTests --coverage",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "start-server-and-test start http://localhost:3000 cypress",
    "e2e:headless": "start-server-and-test start http://localhost:3000 cypress:headless"
  },
  "dependencies": {
    "@apollo/client": "^3.7.15",
    "@apollo/react-hooks": "^4.0.0",
    "@headlessui/react": "^1.7.7",
    "@sentry/browser": "^7.84.0",
    "@sentry/nextjs": "^7.84.0",
    "@sentry/node": "^7.84.0",
    "@types/uuid": "^9.0.1",
    "apollo3-cache-persist": "^0.14.1",
    "axios": "^1.6.0",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.7",
    "deepmerge": "^4.2.2",
    "framer-motion": "^10.16.16",
    "graphql": "^16.8.1",
    "graphql-request": "^5.1.0",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^5.14.0",
    "isomorphic-unfetch": "^4.0.2",
    "jose": "^5.2.0",
    "js-cookie": "^3.0.1",
    "next": "^13.4.4",
    "next-auth": "^4.24.5",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.60.0",
    "tailwind-variants": "^0.1.18",
    "twemoji": "^14.0.2",
    "uuid": "^9.0.0",
    "yaml": "^2.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/introspection": "^4.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-react-apollo": "^4.0.0",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.3",
    "@types/js-cookie": "^3.0.3",
    "@types/node": "^18.16.3",
    "@types/react": "^18.0.26",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.59.9",
    "autoprefixer": "^10.4.17",
    "babel-jest": "^29.3.1",
    "babel-loader": "^9.1.0",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "concurrently": "^8.2.2",
    "cypress": "^13.3.2",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^13.0.7",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.31",
    "prettier": "^3.1.1",
    "pretty-quick": "^3.1.3",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.3.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.0.0",
    "yaml-loader": "^0.8.0"
  }
}
